<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.discardpast.chapter_three.Persistence.Seller" table="Seller" schema="online_shopping">
        <id name="id" column="ID"/>
        <property name="name" column="name"/>
        <property name="tel" column="tel"/>
        <property name="address" column="address"/>
        <property name="wesite" column="wesite"/>
        <property name="star" column="star"/>
        <property name="business" column="business"/>
    </class>
</hibernate-mapping>